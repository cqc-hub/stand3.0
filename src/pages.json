{
	"easycom": {
		"^g-(.*)": "components/g-$1/g-$1.vue"
	},
	"pages": [
		//新增extend扩展属性，选填：
		//-- login：该页面是否要登录，默认false；
		//-- patient：该页面是否要就诊人，默认false；
		{
			"path": "pages/home/home",
			"style": {
				"navigationBarTitleText": "首页"
			}
		},
		{
			"path": "pages/piniaTest/piniaTest",
			"style": {
				"navigationBarTitleText": "uni-app "
			}
		},
		{
			"path": "pages/home/my",
			"style": {
				"navigationBarTitleText": "我的"
			},
			"extend": {
				"login": true,
				"patient": true
			}
		},

		{
			"path": "pages/index/index",
			"style": {
				"navigationBarTitleText": "uni-app"
			}
		},
		{
			"path": "pages/routerTest/routerTest",
			"style": {
				"navigationBarTitleText": "uni-routerTest"
			}
		}
	],
	"globalStyle": {
		"navigationBarTextStyle": "black",
		"navigationBarTitleText": "uni-app",
		"backgroundColor": "#F8F8F8",
		// #ifdef MP-WEIXIN || H5
		"navigationBarBackgroundColor": "#f8f8f8"
		// #endif
	},
	"tabBar": {
		"color": "#666666",
		"selectedColor": "#111111",
		"borderStyle": "black",
		"backgroundColor": "#ffffff",
		"list": [
			{
				"pagePath": "pages/home/home",
				"iconPath": "/static/image/home.svg",
				"selectedIconPath": "/static/image/home_active.svg",
				"text": "首页"
			},
			{
				"pagePath": "pages/home/my",
				"iconPath": "/static/image/my.svg",
				"selectedIconPath": "/static/image/my_active.svg",
				"text": "我的"
			}
		]
	}
}
